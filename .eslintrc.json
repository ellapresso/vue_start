{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
    },
  "env": {
    "browser": false,
    "node": true
    },
  "plugins": ["vue", "eslint-plugin-vue", "eslint-plugin"],
  "rules": {
    "linebreak-style": 0,
    "object-curly-spacing": 0,
    "new-cap": 0,
    "indent": ["error", 2],
    "max-len": ["error", {"code": 160}],
    "require-jsdoc": 0,
    "comma-dangle": 0,
    "one-var": 0,
    "arrow-parens": ["error", "as-needed"],
    "no-extend-native": ["error", {"exceptions": ["String"]}],
    "no-invalid-this": 0
    }
}
